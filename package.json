{
    "name": "prem-kashyap-portfolio",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "description": "Modern React portfolio for Prem Kashyap - Software Developer",
    "author": "Prem Kashyap",
    "keywords": [
        "portfolio",
        "react",
        "vite",
        "cloudflare",
        "ai",
        "machine-learning"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "deploy": "npm run build && wrangler deploy --assets=./dist",
        "deploy:preview": "npm run build && wrangler deploy --assets=./dist --env preview",
        "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write src/**/*.{js,jsx,css,md}",
        "clean": "rm -rf dist node_modules/.cache"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "prettier": "^3.2.5",
        "vite": "^7.1.3",
        "wrangler": "^4.32.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}